{
  "name": "booked-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress": "cypress open",
    "cypress:run": "cypress run -headless",
    "seed": "dotenv -e .env.local tsx src/utils/seedDb.ts",
    "migrate": "npx prisma migrate dev --schema ./prisma/schema.prisma",
    "generate": "npx prisma generate --schema ./prisma/schema.prisma",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "seed:test": "dotenv -e .env.test tsx src/utils/seedDb.ts",
    "test": "DOTENV_CONFIG_PATH=./.env.test jest --setupFiles dotenv/config",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "db:seed": "prisma db seed"
  },
